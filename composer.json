{
    "name": "bitsoftsolutions/laravel-reverb-doctor",
    "description": "A CLI diagnostic tool that performs comprehensive health checks on Laravel Reverb WebSocket configurations",
    "keywords": ["laravel", "reverb", "websocket", "diagnostic", "debug", "doctor", "health-check", "broadcasting"],
    "homepage": "https://github.com/hafizSiddiq7675/laravel-reverb-doctor",
    "license": "MIT",
    "authors": [
        {
            "name": "Hafiz Siddiq",
            "email": "hafiz.siddiq.umer@gmail.com",
            "homepage": "https://github.com/hafizSiddiq7675"
        }
    ],
    "support": {
        "issues": "https://github.com/hafizSiddiq7675/laravel-reverb-doctor/issues",
        "source": "https://github.com/hafizSiddiq7675/laravel-reverb-doctor"
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bitsoftsolutions\\LaravelReverbDoctor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitsoftsolutions\\LaravelReverbDoctor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bitsoftsolutions\\LaravelReverbDoctor\\ReverbDoctorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
